#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/upgrades-core/dist/cli/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/upgrades-core/dist/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/upgrades-core/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/upgrades-core/dist/cli/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/upgrades-core/dist/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/upgrades-core/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.35.0/node_modules:/mnt/c/Users/User/Documents/Github/WAd/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/upgrades-core/dist/cli/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../@openzeppelin+upgrades-core@1.35.0/node_modules/@openzeppelin/upgrades-core/dist/cli/cli.js" "$@"
fi
